<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InfoDosenInformatika">
    <View
        android:layout_width="match_parent"
        android:layout_height="258dp"
        android:background="@drawable/logo_umpar"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:id="@+id/toolbar_dosen"
        />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="315dp"
        android:layout_height="234dp"
        android:background="@drawable/bg_lihat"
        app:layout_constraintCircleRadius="10dp"
        app:layout_constraintTop_toBottomOf="@+id/toolbar_dosen"
        app:layout_constraintBottom_toBottomOf="@id/toolbar_dosen"
        android:elevation="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:id="@+id/bg_dosen"
        />
    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/ci_dosen"
        android:layout_width="73dp"
        android:layout_height="73dp"
        android:src="@drawable/social"
       app:layout_constraintTop_toTopOf="@id/bg_dosen"
        app:layout_constraintBottom_toBottomOf="@id/toolbar_dosen"
        app:layout_constraintLeft_toLeftOf="@id/bg_dosen"
        app:layout_constraintRight_toRightOf="@id/bg_dosen"
        android:elevation="70dp"
        android:layout_marginBottom="70dp"
        />
    <View
        android:layout_width="137dp"
        android:layout_height="26dp"
        android:background="@drawable/bg_nohp"
        app:layout_constraintTop_toBottomOf="@+id/ci_dosen"
        android:elevation="70dp"
        app:layout_constraintLeft_toLeftOf="@id/ci_dosen"
        app:layout_constraintRight_toRightOf="@id/ci_dosen"
        android:layout_marginTop="8dp"
        android:id="@+id/view_nohp"
        />
    <com.google.android.material.textview.MaterialTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="08123456789"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@id/view_nohp"
        app:layout_constraintLeft_toLeftOf="@id/view_nohp"
        app:layout_constraintRight_toRightOf="@id/view_nohp"
        android:elevation="70dp"
        android:layout_marginBottom="8dp"
        android:textSize="16sp"
        android:id="@+id/tv_nohp_informatika"
        />
    <com.google.android.material.textview.MaterialTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Syahirun Alam"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/view_nohp"
        app:layout_constraintLeft_toLeftOf="@id/view_nohp"
        app:layout_constraintRight_toRightOf="@id/view_nohp"
        android:elevation="70dp"
        android:id="@+id/tv_nama_dosen_informatika"
        android:layout_marginTop="8dp"
        />
    <com.google.android.material.textview.MaterialTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2345754"
        android:textSize="15sp"
        app:layout_constraintTop_toBottomOf="@id/tv_nama_dosen_informatika"
        app:layout_constraintLeft_toLeftOf="@id/tv_nama_dosen_informatika"
        app:layout_constraintRight_toRightOf="@id/tv_nama_dosen_informatika"
        android:elevation="70dp"
        android:id="@+id/tv_nbm_informatika"
        android:layout_marginTop="8dp"
        />

    <androidx.appcompat.widget.AppCompatImageButton
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:background="@drawable/copy"
        android:elevation="70dp"
        app:layout_constraintTop_toBottomOf="@id/tv_nbm_informatika"
        app:layout_constraintLeft_toLeftOf="@id/bg_dosen"
        android:layout_marginLeft="61dp"
        android:layout_marginTop="16dp"
        android:id="@+id/btn_copy"
        />
    <androidx.appcompat.widget.AppCompatImageButton
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:background="@drawable/technology"
        app:layout_constraintTop_toBottomOf="@id/tv_nbm_informatika"
        app:layout_constraintLeft_toRightOf="@id/btn_copy"
        app:layout_constraintRight_toLeftOf="@id/btn_telp"
        android:elevation="70dp"
        android:layout_marginTop="16dp"
        android:id="@+id/btn_wa"
        android:layout_marginRight="42dp"
        android:layout_marginLeft="42dp"
        />
    <androidx.appcompat.widget.AppCompatImageButton
        android:layout_width="35dp"
        android:layout_height="35dp"
       android:background="@drawable/communications"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/tv_nbm_informatika"
        app:layout_constraintLeft_toRightOf="@id/btn_wa"
        app:layout_constraintRight_toRightOf="@id/bg_dosen"
        android:elevation="70dp"
        android:id="@+id/btn_telp"
        android:layout_marginRight="50dp"
        />
    <com.google.android.material.textview.MaterialTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Copy"
        app:layout_constraintTop_toBottomOf="@id/btn_copy"
        app:layout_constraintLeft_toLeftOf="@id/btn_copy"
        app:layout_constraintRight_toRightOf="@id/btn_copy"
        android:elevation="60dp"
        />
    <com.google.android.material.textview.MaterialTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chat"
        app:layout_constraintTop_toBottomOf="@id/btn_wa"
        app:layout_constraintLeft_toLeftOf="@id/btn_wa"
        app:layout_constraintRight_toRightOf="@id/btn_wa"
        android:elevation="60dp"
        />
    <com.google.android.material.textview.MaterialTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Telpon"
        app:layout_constraintTop_toBottomOf="@id/btn_telp"
        app:layout_constraintLeft_toLeftOf="@id/btn_telp"
        app:layout_constraintRight_toRightOf="@id/btn_telp"
        android:elevation="60dp"
        />
</androidx.constraintlayout.widget.ConstraintLayout>